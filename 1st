#include <stdio.h>

int main() {
    int num1, num2, sum;

    // Ask the user to enter two integers
    printf("Enter first integer: ");
    scanf("%d", &num1);

    printf("Enter second integer: ");
    scanf("%d", &num2);

    // Calculate the sum
    sum = num1 + num2;

    // Output the result
    printf("Sum = %d\n", sum);

    return 0;
}
